{
  "name": "angular-ngrx-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --disableHostCheck --poll=500",
    "build": "ng build",
    "test": "jest",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test:watch": "jest --watch",
    "extract-translations": "ngx-translate-extract --input ./src --output ./src/assets/i18n/*.json --sort --format-indentation '  ' --format namespaced-json --marker _",
    "ng-high-memory-build": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --prod --build-optimizer --configuration=production"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.2.0",
    "@angular/cdk": "8.1.2",
    "@angular/common": "8.2.0",
    "@angular/compiler": "8.2.0",
    "@angular/core": "8.2.0",
    "@angular/flex-layout": "8.0.0-beta.26",
    "@angular/forms": "8.2.0",
    "@angular/material": "8.1.2",
    "@angular/platform-browser": "8.2.0",
    "@angular/platform-browser-dynamic": "8.2.0",
    "@angular/router": "8.2.0",
    "@auth0/angular-jwt": "3.0.0",
    "@material/layout-grid": "3.1.0",
    "@ngrx/effects": "8.2.0",
    "@ngrx/entity": "8.2.0",
    "@ngrx/router-store": "8.2.0",
    "@ngrx/store": "8.2.0",
    "@ngx-resource/core": "7.0.0",
    "@ngx-resource/handler-ngx-http": "7.0.0",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "core-js": "3.1.4",
    "hammerjs": "2.0.8",
    "ngx-webstorage": "4.0.1",
    "node-sass": "7.0.0",
    "rxjs": "6.5.2",
    "tslib": "1.10.0",
    "zone.js": "0.10.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.802.0",
    "@angular/cli": "8.2.0",
    "@angular/compiler-cli": "8.2.0",
    "@angular/language-service": "8.2.0",
    "@biesbjerg/ngx-translate-extract": "3.0.2",
    "@briebug/jest-schematic": "1.7.0",
    "@ngrx/store-devtools": "8.2.0",
    "@types/jasmine": "3.3.16",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "12.6.9",
    "codelyzer": "5.1.0",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "jest": "24.8.0",
    "jest-preset-angular": "7.1.1",
    "ngrx-store-freeze": "^0.2.4",
    "protractor": "5.4.2",
    "ts-node": "8.3.0",
    "tslint": "5.18.0",
    "typescript": "3.5.3"
  }
}
